{
  "name": "web-push-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": "14.17.3"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css src/**/*.js src/public/favicon.ico src/my_cert.crt src/my_cert.key dist/",
    "start": "node dist/app.js",
    "postinstall": "tsc && npm run copy-files",
    "watch-js": "nodemon dist/app.js",
    "watch-ts": "tsc -w",
    "build": "npm run clean && tsc && npm run copy-files",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase": "^9.2.0",
    "firebase-admin": "^10.0.0",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.38",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.14",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2"
  }
}
